{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "lib": [
            "dom",
            "es2017"
        ],
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./server/dist",
        "baseUrl": "./server/",
        "paths": {
            "*": [
                "node_modules/*",
                "./server/types/*"
            ]
        }
    },
    "include": [
        "./server/src/**/*"
    ]
}
