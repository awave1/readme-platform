{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [true, "never"],
        "indent": [true, "spaces", 2],
        "quotemark":false,
        "ordered-imports":false,
        "curly": false,
        "array-type": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "rulesDirectory": []
}
